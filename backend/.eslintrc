{
  "extends": "standard",
  "plugins": [
    "standard",
    "promise"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "classes": true,
      "modules": true,
      "jsx": true
    }
  }
}
